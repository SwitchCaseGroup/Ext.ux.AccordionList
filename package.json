{
    "name": "Ext.ux.AccordionList",
    "version": "1.1.1",
    "repository": "https://github.com/kawanoshinobu/Ext.ux.AccordionList",
    "license": "MIT",
    "type": "code",
    "creator": "Shinobu Kawano",
    "summary": "Accordion List for Sencha Touch 2",
